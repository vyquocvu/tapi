    "@types/multer": "^2.0.0",
    "@types/node": "^22.18.10",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vercel/node": "^3.2.29",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.18",
