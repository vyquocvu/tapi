    "@tanstack/react-query-devtools": "^5.59.0",
    "@tanstack/react-router": "1.133.21",
    "@tanstack/router-devtools": "1.133.21",
    "@vercel/blob": "^2.0.0",
    "@vitejs/plugin-react": "^4.3.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.545.0",
    "multer": "^2.0.2",
    "prisma": "^6.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tsx": "^4.20.6"
  },
