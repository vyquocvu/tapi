{
  "api::article.article": {
    "uid": "api::article.article",
    "displayName": "Article",
    "singularName": "article",
    "pluralName": "articles",
    "description": "Blog articles with rich content",
    "fields": {
      "title": {
        "type": "string",
        "required": true,
        "maxLength": 255
      },
      "slug": {
        "type": "string",
        "required": true,
        "unique": true,
        "maxLength": 255
      },
      "content": {
        "type": "text",
        "required": true
      },
      "excerpt": {
        "type": "text"
      },
      "published": {
        "type": "boolean",
        "default": false
      },
      "publishedAt": {
        "type": "datetime"
      },
      "viewCount": {
        "type": "integer",
        "default": 0
      },
      "status": {
        "type": "enumeration",
        "values": [
          "draft",
          "published",
          "archived"
        ],
        "default": "draft"
      },
      "authorId": {
        "type": "integer",
        "required": false
      },
      "category": {
        "type": "relation",
        "relationType": "manyToOne",
        "target": "api::category.category"
      }
    },
    "options": {
      "timestamps": true
    }
  },
  "api::category.category": {
    "uid": "api::category.category",
    "displayName": "Category",
    "singularName": "category",
    "pluralName": "categories",
    "description": "Article categories",
    "fields": {
      "name": {
        "type": "string",
        "required": true,
        "maxLength": 100
      },
      "slug": {
        "type": "string",
        "required": true,
        "unique": true,
        "maxLength": 100
      },
      "description": {
        "type": "text"
      },
      "articles": {
        "type": "relation",
        "relationType": "oneToMany",
        "target": "api::article.article"
      }
    },
    "options": {
      "timestamps": true
    }
  },
  "api::city.city": {
    "uid": "api::city.city",
    "displayName": "city",
    "singularName": "city",
    "pluralName": "cities",
    "fields": {
      "name": {
        "type": "string",
        "required": true,
        "unique": false
      },
      "contry": {
        "type": "string",
        "required": true,
        "unique": false
      }
    },
    "options": {
      "timestamps": true,
      "softDelete": false
    }
  }
}